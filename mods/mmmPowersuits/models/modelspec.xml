<models>
    <model file="/mods/mmmPowersuits/models/mps_pantaloons.obj"
           textures="/mods/mmmPowersuits/textures/models/diffuse.png">
        <binding slot="2" target="LeftLeg">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="leg2" name="leg2"/>
        </binding>
        <binding slot="2" target="RightLeg">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="leg1" name="leg1"/>
        </binding>
    </model>
    <model file="/mods/mmmPowersuits/models/mps_boots.obj" textures="/mods/mmmPowersuits/textures/models/diffuse.png">
        <binding slot="3" target="RightLeg">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="boots1" name="boots1"/>
        </binding>
        <binding slot="3" target="LeftLeg">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="boots2" name="boots2"/>
        </binding>
    </model>
    <model file="/mods/mmmPowersuits/models/mps_chest.obj" textures="/mods/mmmPowersuits/textures/models/diffuse.png">
        <binding slot="1" target="Body">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="backpack" name="backpack"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="polySurface36" name="polySurface36"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="belt" name="belt"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="chest_padding" name="chest_padding"/>
            <part defaultcolor="114EFF" defaultglow="true" polygroup="crystal_belt" name="crystal_belt"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="chest_main" name="chest_main"/>
        </binding>
    </model>
    <model file="/mods/mmmPowersuits/models/mps_arms.obj" textures="/mods/mmmPowersuits/textures/models/diffuse.png">
        <binding slot="1" target="RightArm">
            <part defaultcolor="114EFF" defaultglow="true" polygroup="crystal_shoulder_2" name="crystal_shoulder_2"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="arms3" name="arms3"/>
        </binding>
        <binding slot="1" target="LeftArm">
            <part defaultcolor="114EFF" defaultglow="true" polygroup="crystal_shoulder_1" name="crystal_shoulder_1"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="arms2" name="arms2"/>
        </binding>
    </model>
    <model file="/mods/mmmPowersuits/models/mps_helm.obj" textures="/mods/mmmPowersuits/textures/models/diffuse.png">
        <binding slot="0" target="Head">
            <part defaultcolor="114EFF" defaultglow="true" polygroup="visor" name="visor"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="helm_tubes" name="helm_tubes"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="helm_tube_entry2" name="helm_tube_entry2"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="helm_tube_entry1" name="helm_tube_entry1"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="helm_main" name="helm_main"/>
        </binding>
    </model>
</models>